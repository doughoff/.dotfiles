clear      lock 
clear   control
clear      mod1
clear      mod2
clear      mod3
clear      mod4

keycode      66 = Super_L
keycode     133 = Hyper_L

add     control = Control_L Control_R
add        mod1 = Alt_L Alt_R Meta_L
add        mod2 = Num_Lock
add        mod3 = Hyper_L
add        mod4 = Super_L Super_R